{
  "name": "true-soft-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run start_server",
    "start_server": "node ./bin/www",
    "clean": "rm -rf dist/*",
    "moveFiles": "cp -R assets dist/ && cp app/index.html dist/",
    "build": "npm run clean && webpack -p && npm run moveFiles",
    "serve-dev": "webpack-dev-server --content-base dist/ --host 0.0.0.0 --port 3000 --progress --colors"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-cookies": "1.5.8",
    "angular-jwt": "^0.1.9",
    "angular-material": "1.1.3",
    "angular-messages": "1.5.8",
    "angular-moment": "^1.0.1",
    "angular-resource": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-simple-logger": "0.1.x",
    "angular-ui-router": "0.3.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "font-awesome": "4.7.0",
    "font-awesome-webpack": "0.0.5-beta.2",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jquery": "2.2.4",
    "less": "^3.5.3",
    "morgan": "~1.9.0",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-loader": "6.4.0",
    "babel-preset-es2015": "6.22.0",
    "css-loader": "0.26.2",
    "file-loader": "0.10.0",
    "html-loader": "0.4.4",
    "http-server": "0.9.0",
    "json-loader": "0.5.4",
    "ng-annotate": "1.2.1",
    "ng-annotate-loader": "0.2.0",
    "ng-cache-loader": "0.0.22",
    "nginject-loader": "2.1.0",
    "ngtemplate-loader": "1.3.1",
    "node-sass": "4.5.0",
    "replace-in-file": "2.3.2",
    "sass-loader": "4.1.1",
    "sass-loader-once": "0.0.2",
    "style-loader": "0.13.2",
    "svg-loader": "0.0.2",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-clear-console": "1.0.3",
    "webpack-dev-server": "2.4.5",
    "webpack-strip": "0.1.0"
